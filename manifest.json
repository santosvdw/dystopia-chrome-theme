{
    "manifest_version": 3,
    "name": "Dystopia Chrome Theme",
    "short_name": "Dystopia Theme",
    "version": "1.0",
    "description": "Dystopia Chrome Theme, based on the Dystopia theme by @santosvdw",
    "author": "santosvdw",

    "theme": {
        "colors": {
            "frame": [4, 4, 3],
            "frame_incognito": [39, 43, 53],
            "frame_inactive": [39, 43, 53],
            "frame_incognito_inactive": [39, 43, 53],
            "toolbar": [32, 33, 36],
